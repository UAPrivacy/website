{
  "name": "website",
  "dependencies": {
    "uikit": "^3.0.0-rc.24"
  },
  "devDependencies": {
    "parcel-bundler": "^1.10.3",
    "serve": "^8.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UAPrivacy/website.git"
  },
  "scripts": {
    "build": "npm run clear && parcel build index.html --public-url ./",
