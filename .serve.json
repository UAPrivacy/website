{
    "headers": [{
            "source": "**/*.@(jpg|jpeg|gif|png)",
            "headers": [{
                "key": "Cache-Control",
                "value": "max-age=604800"
            }]
        },
        {
            "source": "**/*.@(js|css|html)",
            "headers": [{
                "key": "Vary",
                "value": "Accept-Encoding"
            }]
        },
        {
            "source": "https://fonts.googleapis.com",
            "headers": [{
                "key": "Cache-Control",
                "value": "max-age=604800"
            }]
        }
    ]
}