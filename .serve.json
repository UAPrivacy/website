{
	"headers": [{
			"source": "**/*.@(jpg|png)",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=31536000"
			}]
		},
		{
			"source": "**/*.@(js|css|html)",
			"headers": [{
				"key": "Vary",
				"value": "Accept-Encoding"
			}]
		},
		{
			"source": "https://fonts.googleapis.com",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=31536000"
			}]
		},
		{
			"source": "https://use.fontawesome.com",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=31536000"
			}]
		}
	]
}